      \tl_if_eq:NnTF \l_@@_bib_tool_tl { bibtex }
        {
          \cs_set:Npn \bibsection { }
          \rmfamily \zihao { 5 }
          \dim_set:Nn \baselineskip { 20pt }
          \bibliography { \l_@@_bib_file_clist }
        }
        {
          \defbibheading { bibliography } [ ] { }
          \cs_set:Npn \bibfont
            {
